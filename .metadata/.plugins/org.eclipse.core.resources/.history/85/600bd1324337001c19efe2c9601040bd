import java.awt.Color;
import javax.swing.Timer;

public class GrafikBeispiel extends StandardAnwendung implements ActionListener{
	public static void main(String[] args) {
		starteAnwendung();
	}
	
	public GrafikBeispiel() {
		super("Fenstertitel", 500, 600);
		this.setBackground(Color.YELLOW);
		Timer frame = new Timer(40, this);
		frame.start();
	}
	
	
}
